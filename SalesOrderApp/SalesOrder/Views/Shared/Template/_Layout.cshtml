
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Sales Order App</title>

    <partial name="Template/_Styles" />

    

</head>
<body class="hold-transition sidebar-mini ">
    <div class="wrapper">

       @* <div class="spinner">
            <div class="center-div">
                <div class="inner-div">
                    <div class="loader"></div>
                </div>
            </div>
        </div>*@

        @*<partial name="Template/_TopNavigation" />*@
        @await Component.InvokeAsync("TopMenuWidget")
        <!-- Main Sidebar Container -->
        @*<partial name="Template/_MainNavigation" />*@
        @await Component.InvokeAsync("MainMenuWidget")


        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
            <div class="p-3">
                <h5>Title</h5>
                <p>Sidebar content</p>
            </div>
        </aside>
        <!-- /.control-sidebar -->

        <partial name="Template/_Footer" />
    </div>
    <!-- ./wrapper -->

    <partial name="Template/_Scripts" />
    @RenderSection("Scripts", required: false)
</body>
</html>
